{
  "name": "vue3-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint *.config.{js,ts} {src,tests}/**/*.{js,ts,vue} --report-unused-disable-directives --max-warnings 0 --fix",
    "test:unit": "jest --passWithNoTests",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e": "playwright test --pass-with-no-tests",
    "test:e2e:report": "playwright show-report",
    "build": "vue-tsc && vite build --debug",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:test": "vue-tsc && vite build --mode test",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build:prod": "vue-tsc && vite build --mode prod",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@sentry/vite-plugin": "^2.22.4",
    "@sentry/vue": "^8.30.0",
    "axios": "^1.7.2",
    "element-plus": "^2.7.5",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@eslint/js": "^9.4.0",
    "@playwright/test": "^1.45.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^20.14.9",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.19",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "miragejs": "^0.2.0-alpha.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "sass": "^1.77.5",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.6"
  },
  "packageManager": "yarn@4.3.0"
}
