{
  "name": "infra",
  "version": "1.0.0",
  "description": "manage cdk as a local dependency",
  "scripts": {
    "cdk": "cd $INIT_CWD && cdk",
    "cdk:clear": "rm -rf cdk.out",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy",
    "cdk-assets": "cd $INIT_CWD && cdk-assets"
  },
  "dependencies": {
    "aws-cdk": "^2.158.0",
    "cdk-assets": "^2.151.38"
  }
}
